    "@packages/launcher": "0.0.0-development",
    "@packages/network": "0.0.0-development",
    "@packages/root": "0.0.0-development",
    "@packages/server": "0.0.0-development",
    "@packages/socket": "0.0.0-development",
    "@packages/ts": "0.0.0-development",
    "@types/chai": "4.2.15",
    "@types/mocha": "9.1.0",
    "babel-loader": "9.1.3",
    "base64url": "^3.0.1",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "cachedir": "2.3.0",
    "chai": "1.10.0",
