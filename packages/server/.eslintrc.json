{
  "extends": [
    "../../.eslintrc.js"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "cypress/globals": true
  },
  "plugins": [
    "cypress"
  ],
  "overrides": [
    {
      "files": [
        "./lib/graphql/entities/**/*.ts"
      ],
      "rules": {
        "@typescript-eslint/explicit-function-return-type": [
          "error"
        ]
      }
    }
  ]
}
