{
  "name": "@packages/electron",
  "version": "0.0.0",
  "electronVersion": "1.8.2",
  "private": true,
  "main": "index.js",
  "scripts": {
    "postinstall": "echo '@packages/electron needs: npm run build'",
    "prestart": "npm run check-deps-pre",
    "start": "./bin/cypress-electron",
    "pretest": "npm run check-deps-pre",
    "test": "mocha --require @packages/coffee/register test/**/*.coffee",
    "prebuild": "npm run check-deps-pre",
    "build": "node ./bin/cypress-electron --install",
    "check-deps": "node ../../scripts/check-deps.js --verbose",
    "check-deps-pre": "npm run check-deps -- --prescript",
    "clean-deps": "rm -rf node_modules"
  },
  "bin": {
    "cypress-electron": "./bin/cypress-electron"
  },
  "files": [
    "dist",
    "lib"
  ],
  "devDependencies": {
    "@cypress/releaser": "0.2.2",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "@cypress/icons": "^0.7.0",
    "bluebird": "^3.5.2",
    "debug": "^4.1.0",
    "electron-packager": "12.2.0",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0"
  }
}
