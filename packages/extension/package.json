    "coffeeify": "3.0.1",
    "coffeescript": "1.12.7",
    "cross-env": "6.0.3",
    "eol": "0.9.1",
    "fs-extra": "8.1.0",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-rename": "1.4.0",
    "mocha": "3.5.3",
    "mock-require": "3.0.3",
    "rimraf": "2.6.3",
