    "bin-up": "^1.0.0",
    "bluebird": "^3.4.0",
    "bootstrap-sass": "3.3.5",
    "classnames": "^2.2.5",
    "fira": "cypress-io/fira#fb63362742eea8cdce0d90825ab9264d77719e3d",
    "font-awesome": "4.7",
    "gravatar": "^1.6.0",
    "human-interval": "^0.1.6",
    "istanbul": "^0.4.3",
    "lodash": "^4.13.1",
    "md5": "^2.1.0",
    "mobx": "^3.1.11",
    "mobx-react": "^4.2.1",
    "mobx-react-devtools": "^4.2.0",
    "moment": "^2.15.1",
    "prop-types": "^15.5.10",
    "rc-collapse": "^1.6.11",
    "react": "^15.6.1",
    "react-bootstrap-modal": "3.0.1",
