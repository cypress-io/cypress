{
  "name": "@packages/desktop-gui",
  "version": "0.0.0",
  "main": "lib/gui.js",
  "private": true,
  "scripts": {
    "postinstall": "echo '@packages/desktop-gui needs: npm run build'",
    "build": "node ./scripts/build-dev.js",
    "build-prod": "node ./scripts/build-prod.js",
    "watch": "node ./scripts/watch.js",
    "clean": "zunder clean",
