{
  "name": "@packages/desktop-gui",
  "version": "0.0.0-development",
  "private": true,
  "main": "lib/gui.js",
  "scripts": {
    "build": "webpack",
    "build-prod": "cross-env NODE_ENV=production yarn build",
    "clean-deps": "rimraf node_modules",
    "cypress:open": "cross-env TZ=America/New_York node ../../scripts/cypress open --project .",
    "cypress:open:ct": "cross-env TZ=America/New_York node ../../scripts/cypress open --ct --project .",
    "cypress:run": "cross-env TZ=America/New_York node ../../scripts/cypress run --project .",
    "cypress:run:ct": "cross-env TZ=America/New_York node ../../scripts/cypress run --ct --project .",
    "dev": "yarn start-test 5005 cypress:open",
    "postinstall": "echo '@packages/desktop-gui needs: yarn build'",
    "start": "http-server -p 5005 dist",
    "watch": "yarn build --watch --progress"
  },
  "devDependencies": {
    "@cypress/icons": "0.7.0",
    "@cypress/json-schemas": "5.35.1",
    "@cypress/react": "0.0.0-development",
    "@cypress/react-tooltip": "0.5.3",
    "@cypress/webpack-preprocessor": "0.0.0-development",
    "@fontsource/fira-sans": "4.3.0",
    "@fontsource/poppins": "4.3.0",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@packages/web-config": "0.0.0-development",
    "@popperjs/core": "2.9.2",
    "@reach/dialog": "0.10.5",
    "bluebird": "3.5.3",
    "bootstrap-sass": "3.4.1",
    "classnames": "2.3.1",
    "cross-env": "6.0.3",
    "cypress-multi-reporters": "1.4.0",
    "cypress-real-events": "1.4.0",
    "dayjs": "^1.9.3",
    "gravatar": "1.8.0",
    "human-interval": "1.0.0",
    "lodash": "4.17.21",
    "markdown-it": "11.0.0",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "mobx-react-devtools": "6.1.1",
    "prop-types": "15.7.2",
    "rc-collapse": "2.0.0",
    "react": "16.8.6",
    "react-bootstrap-modal": "4.2.0",
    "react-dom": "16.8.6",
    "react-inspector": "5.0.1",
    "react-loader": "2.4.7",
    "react-popper": "2.2.5",
    "react-select": "3.1.0",
    "react-transition-group": "4.4.1",
    "rimraf": "3.0.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.2"
  },
  "files": [
    "dist",
    "!lib/**/*_spec.jsx"
  ]
}
