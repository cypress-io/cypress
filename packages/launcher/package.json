{
  "name": "@packages/launcher",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "tsc --project .",
    "build-js": "tsc --project .",
    "build-prod": "npm run build",
    "clean": "node scripts/clean.js || true",
    "clean-deps": "rm -rf node_modules",
    "clean-js": "npm run clean",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run unit",
    "unit": "mocha --reporter mocha-multi-reporters --reporter-options configFile=../../mocha-reporter-config.json"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "debug": "4.1.1",
    "execa": "1.0.0",
    "fs-extra": "8.1.0",
    "lodash": "4.17.15",
    "plist": "2.1.0",
    "ramda": "0.24.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "mocha": "3.5.3",
    "shelljs": "0.8.3",
    "sinon": "2.4.1",
    "sinon-chai": "3.3.0",
    "typescript": "3.5.3"
  },
  "files": [
    "lib"
  ],
  "types": "../ts/index.d.ts"
}
