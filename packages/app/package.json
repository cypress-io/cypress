    "dayjs": "^1.9.3",
    "disparity": "^3.0.0",
    "engine.io-client": "3.5.2",
    "fuzzysort": "^1.1.4",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "human-interval": "1.0.0",
    "javascript-time-ago": "2.3.8",
