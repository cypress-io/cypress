{
  "extends": "../ts/tsconfig.json",
  "include": [
    "src",
  ],
  "compilerOptions": {
    "strict": true,
    "allowJs": false,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noImplicitReturns": false,
    "noUncheckedIndexedAccess": true,
    "ignoreDeprecations": "5.0"
  }
}