{
  "name": "@packages/reporter",
  "version": "0.0.0",
  "private": true,
  "main": "lib/reporter.js",
  "browser": "src/main",
  "scripts": {
    "postinstall": "echo '@packages/reporter needs: npm run build'",
    "prebuild": "rebuild-node-sass",
    "build": "node ./scripts/build-dev.js",
    "build-prod": "node ./scripts/build-prod.js",
    "watch": "node ./scripts/watch.js",
    "clean": "zunder clean",
    "clean-deps": "rm -rf node_modules",
    "test": "node ./scripts/test.js",
    "lint": "bin-up eslint --fix lib/*.js scripts/*.js src/*.js* src/**/*.js*"
  },
  "files": [
    "dist"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env",
            "react",
            "stage-1"
          ],
          "plugins": [
            "add-module-exports",
            "transform-decorators-legacy"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@cypress/react-tooltip": "0.3.1",
    "@cypress/releaser": "0.1.12",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bin-up": "^1.1.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.8.0",
    "classnames": "^2.2.5",
    "css-element-queries": "^0.3.2",
    "enzyme": "^2.9.1",
    "font-awesome": "^4.6.3",
    "jsdom": "^9.4.1",
    "lodash": "4.17.4",
    "markdown-it": "^6.1.1",
    "mobx": "^3.1.15",
    "mobx-react": "^4.2.1",
    "prop-types": "^15.5.9",
    "react": "15.6.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "15.6.2",
    "react-test-renderer": "15.6.2",
    "rebuild-node-sass": "^1.1.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "zunder": "5.5.2"
  }
}
