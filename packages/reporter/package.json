{
  "name": "@packages/reporter",
  "version": "0.0.0",
  "private": true,
  "main": "src/main.jsx",
  "scripts": {
    "build": "webpack",
    "build-prod": "yarn build",
    "clean-deps": "rm -rf node_modules",
    "cypress:open": "node ../../scripts/cypress open --project .",
    "cypress:run": "node ../../scripts/cypress run --project .",
    "postinstall": "echo '@packages/reporter needs: yarn build'",
    "lint": "eslint --fix lib/*.js src/*.js* src/**/*.js*",
    "test": "yarn test-unit",
    "test-debug": "yarn test-unit --inspect-brk=5566",
    "test-unit": "mocha 'src/**/*.spec.*'",
    "test-watch": "yarn test-unit --watch",
    "watch": "yarn build --watch --progress"
  },
  "devDependencies": {
    "@cypress/react-tooltip": "0.5.3",
    "@types/enzyme": "3.9.1",
    "chai": "3.5.0",
    "chai-enzyme": "1.0.0-beta.1",
    "classnames": "2.2.6",
    "css-element-queries": "1.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "6.1.0",
    "font-awesome": "4.7.0",
    "jsdom": "14.1.0",
    "lodash": "4.17.15",
    "markdown-it": "6.1.1",
    "mobx": "5.11.0",
    "mobx-react": "5.4.4",
    "mocha": "6.2.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "sinon": "7.0.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.2"
  },
  "files": [
    "lib"
  ]
}
