  "module": "es2020",
  "workspaces": {
    "nohoist": [
      "@toycode/markdown-it-class",
      "patch-package"
    ]
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@testing-library/cypress/add-commands",
        "@urql/core",
        "@urql/devtools",
        "@urql/exchange-execute",
        "@urql/exchange-graphcache",
        "@urql/vue",
        "@vue/test-utils",
        "@vueuse/core",
        "dedent",
        "fake-uuid",
        "floating-vue",
        "graphql",
        "graphql-relay",
        "graphql/jsutils/Path",
        "gravatar",
        "human-interval",
        "lodash",
        "shiki",
        "vue",
        "vue-i18n",
        "vue-toastification",
        "wonka"
      ]
    }
  }
}