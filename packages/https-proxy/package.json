    "test-watch": "cross-env NODE_ENV=test mocha --watch"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "debug": "4.1.1",
    "fs-extra": "8.1.0",
    "lodash": "4.17.15",
    "node-forge": "0.9.0",
    "proxy-from-env": "1.0.0",
    "semaphore": "1.1.0"
  },
  "devDependencies": {
    "@cypress/debugging-proxy": "2.0.1",
    "@packages/network": "*",
    "@packages/ts": "*",
    "chai": "3.5.0",
    "cross-env": "6.0.3",
    "mocha": "3.5.3",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "sinon": "1.17.7",
    "sinon-as-promised": "4.0.3",
    "sinon-chai": "3.3.0",
    "ssl-root-cas": "1.3.1",
    "supertest": "4.0.2"
  },
  "files": [
    "lib"
