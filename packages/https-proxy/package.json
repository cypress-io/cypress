    "http-mitm-proxy": "^0.5.1",
    "request": "^2.72.0",
    "request-promise": "^3.0.0",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "debug": "^2.6.8",
    "fs-extra": "^0.30.0",
    "lodash": "4.17.11",
    "node-forge": "^0.6.39",
