    "test-watch": "cross-env NODE_ENV=test bin-up mocha --watch",
    "https": "node https.js"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@cypress/releaser": "0.1.12",
    "bin-up": "^1.1.0",
