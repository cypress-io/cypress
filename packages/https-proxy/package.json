{
  "name": "@packages/https-proxy",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean-deps": "rm -rf node_modules",
    "https": "node https.js",
    "regenerate:certs": "cd ./test/helpers/certs && ./regenerate-certs.sh",
    "start": "node index.js",
    "test": "yarn test-unit",
    "test-debug": "yarn test-unit --inspect-brk=5566",
