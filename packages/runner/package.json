{
  "name": "@packages/runner",
  "version": "0.0.0-development",
  "private": true,
  "browser": "src/index.js",
  "scripts": {
    "build": "node ../../scripts/run-webpack",
    "build-prod": "cross-env NODE_ENV=production yarn build",
    "clean-deps": "rimraf node_modules",
    "cypress:open": "node ../../scripts/cypress open",
    "cypress:run": "node ../../scripts/cypress run",
    "postinstall": "echo '@packages/runner needs: yarn build'",
    "test": "yarn test-unit",
    "test-debug": "yarn test-unit --inspect-brk=5566",
    "test-unit": "mocha --config test/.mocharc.json src/**/*.spec.* --exit",
    "test-watch": "yarn test-unit --watch",
    "watch": "node ../../scripts/run-webpack --watch --progress"
  },
  "devDependencies": {
    "@cypress/react-tooltip": "0.5.3",
    "@fontsource/mulish": "4.3.0",
    "@fontsource/open-sans": "4.3.0",
    "@fortawesome/fontawesome-free": "6.0.0",
    "@packages/driver": "0.0.0-development",
    "@packages/icons": "0.0.0-development",
    "@packages/network": "0.0.0-development",
    "@packages/reporter": "0.0.0-development",
    "@packages/socket": "0.0.0-development",
    "@packages/web-config": "0.0.0-development",
    "babel-plugin-prismjs": "1.0.2",
    "bluebird": "3.5.3",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chalk": "2.4.2",
    "classnames": "2.3.1",
    "common-tags": "1.8.0",
    "cross-env": "6.0.3",
    "debug": "^4.3.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jquery": "3.1.1",
    "lodash": "^4.17.21",
    "mobx": "5.15.4",
    "mobx-react": "6.1.8",
    "mocha": "7.0.1",
    "prismjs": "1.21.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rimraf": "3.0.2",
    "sinon": "7.5.0",
    "snap-shot-core": "10.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "3.3.2"
  },
  "files": [
    "dist"
  ]
