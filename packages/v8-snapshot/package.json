{
  "name": "@packages/v8-snapshot",
  "version": "0.9.0",
  "description": "Various utils related to create v8 snapshots for electron apps.",
  "private": false,
  "main": "dist/v8-snapshot.js",
  "scripts": {
    "build": "tsc && (rimraf ./dist/blueprint && cpr ./src/blueprint ./dist/blueprint)",
    "build-prod": "yarn build",
    "check-ts": "tsc --noEmit && yarn -s tslint",
    "clean-deps": "rimraf node_modules",
    "clean": "rimraf dist",
    "test": "yarn test-unit",
    "test-unit": "mocha -r @packages/ts/register --config ./test/.mocharc.js",
    "tslint": "tslint --config ../ts/tslint.json --project .",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@packages/electron-mksnapshot": "0.0.0-development",
    "@packages/packherd": "0.0.0-development",
    "@packages/snapbuild": "0.0.0-development",
    "ansi-colors": "^4.1.1",
    "convert-source-map": "^1.8.0",
    "debug": "^4.1.1",
    "resolve-from": "^5.0.0",
    "source-map-js": "^0.6.2",
    "supports-color": "^8.1.1",
    "terser": "^5.3.2",
    "tslib": "^2.0.1",
    "worker-nodes": "^2.3.0"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "mocha": "7.0.1",
    "rimraf": "^3.0.2",
    "snap-shot-it": "7.9.3",
    "spok": "^1.3.0",
    "stealthy-require": "^1.1.1"
  },
  "files": [
    "dist/*",
    "src/v8-snapshot.ts"
  ],
  "types": "src/v8-snapshot.ts"
}
