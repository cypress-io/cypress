      "npm/*",
      "tooling/*",
      "system-tests",
      "scripts"
    ],
    "nohoist": [
      "**/webpack-preprocessor/babel-loader",
      "**/webpack-preprocessor/**/merge-source-map",
      "**/webpack-preprocessor/**/patch-package",
      "**/webpack-batteries-included-preprocessor/ts-loader",
      "**/jest",
      "**/jest*",
      "**/expect"
    ]
  },
  "lint-staged": {
