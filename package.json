{
  "name": "eslint-plugin-cypress-dev",
  "version": "0.0.0-development",
  "description": "Common ESLint rules shared by Cypress packages",
  "main": "index.js",
  "author": "Chris Breiding (chris@cypress.io)",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "cypress"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cypress-io/eslint-plugin-cypress-dev.git"
  },
  "bugs": {
    "url": "https://github.com/cypress-io/eslint-plugin-cypress-dev/issues"
  },
  "homepage": "https://github.com/cypress-io/eslint-plugin-cypress-dev#readme",
  "peerDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": ">= 3.2.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.2.1"
  },
  "devDependencies": {
    "@types/yargs": "^13.0.0",
    "condition-circle": "1.5.0",
    "github-post-release": "1.13.1",
    "husky": "^0.14.3",
    "semantic-release": "8.2.0",
    "simple-commit-message": "3.3.1"
  },
  "scripts": {
    "postinstall": "node ./deprecated --renamed @cypress/eslint-plugin-dev --message 'you may also need to rename cypress-dev -> @cypress/dev in your .eslintrc'",
    "lint": "eslint --fix *.js",
    "precommit": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish --access public && semantic-release post"
  },
  "release": {
    "verifyConditions": "condition-circle",
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "yargs": "^13.2.4"
  }
}
