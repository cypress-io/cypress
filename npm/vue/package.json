{
  "name": "@cypress/vue",
  "version": "0.0.0-development",
  "description": "Browser-based Component Testing for Vue.js with Cypress.io ✌️🌲",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-prod": "yarn build",
    "cy:open": "node ../../scripts/cypress.js open-ct --project ${PWD}",
    "cy:run": "node ../../scripts/cypress.js run-ct --project ${PWD}",
    "test": "yarn cy:run",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@cypress/webpack-dev-server": "0.0.0-development",
    "@vue/test-utils": "^2.0.0-rc.1",
    "unfetch": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "@babel/preset-env": "7.9.5",
    "@cypress/code-coverage": "3.8.1",
    "@intlify/vue-i18n-loader": "2.0.0-rc.1",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/compiler-sfc": "^3.0.5",
    "axios": "0.19.2",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "css-loader": "3.4.2",
    "cypress": "0.0.0-development",
    "eslint-plugin-vue": "^6.2.2",
    "mocha": "7.1.1",
    "tailwindcss": "1.1.4",
    "typescript": "3.9.6",
    "vue": "3.0.5",
    "vue-i18n": "9.0.0-rc.6",
    "vue-loader": "16.1.2",
    "vue-router": "^4.0.0",
    "vue-style-loader": "4.1.2",
    "vuex": "^4.0.0",
    "webpack": "4.42.0"
  },
  "peerDependencies": {
    "babel-loader": "8",
    "cypress": ">=6.4.0",
    "vue": "3.x"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=8"
  },
  "types": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress.git"
  },
  "homepage": "https://on.cypress.io/component-testing",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/cypress-io/cypress/issues/new?assignees=&labels=npm%3A%20%40cypress%2Fvue&template=1-bug-report.md&title=",
  "keywords": [
    "cypress",
    "vue"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "ciJobs": [
    "npm-vue"
  ]
}
