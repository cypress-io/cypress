{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "allowJs": false,
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "types": [
      "cypress",
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true
  },
  "include": [
    "lib/**/*.ts"
  ]
}
