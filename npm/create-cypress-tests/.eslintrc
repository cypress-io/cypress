{
  "plugins": [
    "cypress",
    "@cypress/dev"
  ],
  "extends": [
    "plugin:@cypress/dev/general",
    "plugin:@cypress/dev/tests"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "no-console": "off",
    "mocha/no-global-tests": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
