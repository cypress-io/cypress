    "@types/webpack-dev-server": "^4.0.0",
    "chai": "^4.2.0",
    "html-webpack-plugin": "4.x",
    "mocha": "^8.1.3",
    "snap-shot-it": "7.9.3",
    "speed-measure-webpack-plugin": "1.4.2",
    "typescript": "^4.2.3",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^4.0.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": ">=4",
    "webpack": ">=4",
