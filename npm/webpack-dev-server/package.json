{
  "name": "@cypress/webpack-dev-server",
  "version": "0.0.0-development",
  "description": "Launches Webpack Dev Server for Component Testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-prod": "tsc",
    "test": "node ./test-wds-3.js",
