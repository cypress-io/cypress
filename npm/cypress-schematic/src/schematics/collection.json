{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Adds Cypress to an Angular project.",
      "factory": "./ng-add/index",
      "schema": "./ng-add/schema.json"
    },
    "spec": {
      "description": "Create a single spec file",
      "factory": "./ng-generate/spec/index",
      "schema": "./ng-generate/spec/schema.json"
    },
    "specs-ct": {
      "description": "Create spec files for all Angular components",
      "factory": "./ng-generate/specs-ct/index",
      "schema": "./ng-generate/specs-ct/schema.json"
    }
  }
}
